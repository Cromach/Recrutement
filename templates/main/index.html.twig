{% extends 'base.html.twig' %}

{% block title %}Galerie d'image - Recrutement Home-Viewer{% endblock %}

{% block stylesheets %}
<link href="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-Vkoo8x4CGsO3+Hhxv8T/Q5PaXtkKtu6ug5TOeNV6gBiFeWPGFN9MuhOf23Q9Ifjh" crossorigin="anonymous">
<link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">
<style>
.pull-right
{
  float: right!important;
}
.gallery
{
  display: flex;
  flex-wrap: wrap;
}
.gallery .image, .gallery .upload-box-container
{
  flex-basis: 24%;
  text-align: center;
  margin: 5px 0.5%;
  box-shadow: 1px 5px 10px -2px;
  position: relative;
}
.gallery .image img
{
  max-width: 100%;
}
.gallery .image p
{
  position: absolute;
  bottom: 0;
  margin: 0;
  left: 0;
  right: 0;
}
.upload-box
{
  background-color: #eee;
  padding: 10px;
}
</style>
{% endblock %}

{% block body %}
<div class="card">
  <div class="card-header">
    <div class="pull-right">
      <select class="form-control" id="galleryChoice">
        <option>Galerie principale</option>
        <option>Autre galerie(extra)</option>
      </select>
    </div>
    Galerie d'images - Recrutement Home-Viewer
  </div>
  <div class="card-body">
    <div class="gallery">
      <div class="image">
        <img src="data:image/jpeg;base64,"/>
        <p>Mock Image #1</p>
      </div>
      <div class="image">
        <img src="data:image/jpeg;base64,"/>
        <p>Mock Image #2</p>
      </div>
      <div class="upload-box-container">
        <div class="upload-box drop-zone">
          <i class="fa fa-cloud-upload fa-5x"></i><br>
          Drag and drop pictures here<br>or<br>
          <label class="btn btn-primary">
            Choose files <input id="file" type="file" hidden multiple="multiple">
          </label>
        </div>
      </div>
    </div>
  </div>
</div>
{% endblock %}

{% block javascripts %}
<script
  src="https://code.jquery.com/jquery-3.4.1.min.js"
  integrity="sha256-CSXorXvZcTkaix6Yvo6HppcZGetbYMGWSFlBw8HfCJo="
  crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.4.1/js/bootstrap.bundle.min.js" integrity="sha384-6khuMg9gaYr5AxOqhkVIODVIvm9ynTT5J4V1cfthmT+emCG6yVmEZsRHdxlotUnm" crossorigin="anonymous"></script>
{% endblock %}
